package com.zebamentoring.Employee;

public class Employee {
	String name;
	String department;

	Employee() {
		super();
		name = "Zeba";
		department = "Dev";
	}

	Employee(String n, String d) {
		n = name;
		d = department;
	}

	public void displayDetails() {
		System.out.println(name + "\n" + department);
	}
}
